{
  "compilerOptions": {
    "module": "NodeNext",
    "skipLibCheck": true,
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "target": "ES2020",
    "sourceMap": true,
    "outDir": "lib",
    "baseUrl": "src",
    "sourceRoot": "src",
    "paths": {
      "@/*": ["*"],
      "@interfaces/*": ["interfaces/*"],
      "@functions/*": ["functions/*"],
      "@assets/*": ["assets/*"],
      "@utils/*": ["utils/*"],
      "@config": ["config.json"]
    },
  }, "include": ["src/**/*"]
}