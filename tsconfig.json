{
  "compilerOptions": {
    "module": "NodeNext",
    "skipLibCheck": true,
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "declaration": false,
    "resolveJsonModule": true,
    "target": "ES2020",
    "sourceMap": true,
    "outDir": "lib",
    "baseUrl": ".",
    "sourceRoot": "src",
    "paths": {
      "@/*": ["src/*"],
      "@config": ["src/config.json"]
    },
  }, "include": ["src/**/*", "dashboard"]
}